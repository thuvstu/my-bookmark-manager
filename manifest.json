{
  "manifest_version": 3,
  "name": "Browser & YouTube Master Tool",
  "version": "5.0",
  "description": "ブックマーク/履歴の完全管理とYouTube高評価のバックアップ",
  "permissions": [
    "bookmarks",
    "readingList",
    "downloads",
    "history",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}